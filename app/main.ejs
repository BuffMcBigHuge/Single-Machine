<main class="" id="wrapper" ng-controller="InitCtrl">

    <!-- ============================================
    HEADER SECTION
    =============================================== -->
    <div id="top-nav" style="margin-bottom:50px" ng-cloak ng-show="isValid()"></div>

    <nav class="header navbar-fixed-top" ng-cloak ng-show="isValid()">
        <div class="container-fluid blue-custom">

            <div style="position:absolute;">
                <a class="navbar-brand" href="#">
                    <span class="white-text t-5x avenir-font">SINGLE MACHINE</span>
                </a>
                <span class="light white-text pull-left" style="height:50px;padding-top:15px;">BETA</span>
            </div>

            <div class="dropdown pull-right blue-custom">
                <button type="button" class="btn t-4x transparent navbar-toggle collapsed dropdown-toggle" data-toggle="dropdown" data-target="#bs-example-navbar-collapse-1" aria-expanded="false" style="margin-top:5px;margin-bottom:5px;">
                    <i class="fa fa-bars white-text"></i>
                </button>
                <ul class="dropdown-menu-right text-center bottom-shadow dropdown-menu" role="menu">
                    <li><a class="blue-text-custom" href="/portfolio">PORTFOLIO</a></li>
                    <li><a class="blue-text-custom" href="/list">USER LIST</a></li>
                    <li><a class="blue-text-custom" href="/about">ABOUT</a></li>
                    <li class="divider"></li>
                    <li ng-if="isAuthenticated()"><a class="dropdown-logout" href="/logout">LOGOUT</a></li>
                </ul>
            </div>

            <div class="collapse navbar-collapse pull-right">
                <ul class="nav navbar-nav">
                    <li><a href="/list"><button class="btn btn-block transparent white-text regular t-1x">USER LIST</button></a></li>
                    <li class="pt-1x"><span class="white-text t-7x"> | </span></li>
                    <li><a href="/portfolio" class="no-padding-right"><button class="btn btn-block transparent white-text regular t-1x">PORTFOLIO</button></a></li>
                    <li><a href="/about" class="no-padding-left"><button class="btn btn-block transparent white-text regular t-1x">ABOUT</button></a></li>
                    <li><a href="/logout" class="no-padding-left"><button class="btn btn-block transparent white-text regular t-1x">LOGOUT</button></a></li>
                </ul>
            </div>

        </div>
    </nav>

    <!-- ============================================
    MAIN CONTENT SECTION
    =============================================== -->
    <div ui-view></div>
    <i ng-hide="isLoaded()" class="fa fa-spinner grey-text-custom-3 fa-pulse fa-1x fa-fw" style="margin-top:200px;"></i>

    <!-- ============================================
    FOOTER SECTION
    =============================================== -->

    <footer ng-cloak class="text-center grey-light grey-text-custom-3" style="padding:50px 0 50px 0" ng-show="!noFooter">

        <div ng-hide="deviceDetectorValue.browser == 'firefox'">
            <div class="center-block grey-text-shimmer" style="width:500px;">
                <h1 class="footer-text">SINGLE MACHINE</h1>
            </div>
        </div>

        <div>
            <div ng-hide="deviceDetectorValue.browser != 'firefox'" class="center-block" style="width:500px">
                <h1 class="footer-text">SINGLE MACHINE</h1>
            </div>
        </div>

        <p class="center-block" style="max-width:400px;width:100%">The content of this project itself is licensed under the Creative Commons Attribution 3.0 license, and the underlying source code used to format and display that content is licensed under the MIT license.</p>

        <div style="margin-top:50px">
            <p>2017 BuffMcBigHuge <a target="_blank" href="https://github.com/BuffMcBigHuge/Single-Machine/"><i class="fa fa-github grey-text-custom ml-1x"></i></a></p>
        </div>

        <div device-detector id="os-windows"></div>
        <div device-detector id="os-mac"></div>
        <div device-detector id="os-ios"></div>
        <div device-detector id="os-android"></div>
        <div device-detector id="os-linux"></div>
        <div device-detector id="os-unix"></div>
        <div device-detector id="os-firefoxos"></div>
        <div device-detector id="browser-chrome"></div>
        <div device-detector id="browser-firefox"></div>
        <div device-detector id="browser-safari"></div>
        <div device-detector id="browser-opera"></div>
        <div device-detector id="browser-ie"></div>
        <div device-detector id="device-android"></div>
        <div device-detector id="device-ipad"></div>
        <div device-detector id="device-iphone"></div>
        <div device-detector id="device-ipod"></div>
        <div device-detector id="device-blackberry"></div>
        <div device-detector id="device-firefoxos"></div>
    </footer>

</main>

